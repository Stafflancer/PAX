{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/cta-icon-content",
	"title": "CTA - Icon + Content",
	"description": "A custom CTA - Icon + Content block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "text",
	"keywords": [ "cta", "icon", "content" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "cta-icon-content.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./cta-icon-content.css"
}