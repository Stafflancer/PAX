{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/side-by-side-image",
	"title": "Side by Side (Image)",
	"description": "A custom Side by Side (Image) block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "format-image",
	"keywords": [ "side", "by", "image"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "side-by-side-image.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./side-by-side-image.css"
}