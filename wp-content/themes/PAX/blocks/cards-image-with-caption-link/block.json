{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/cards-image-with-caption-link",
    "title": "Cards - Image with Caption Link",
    "description": "A custom Cards - Image with Caption Link block.",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "index-card",
    "keywords": [ "cards", "image", "with", "caption", "link" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "cards-image-with-caption-link.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "color": {
            "background": false,
            "link": false,
            "text": true
        },
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": true
    },
    "style": "file:./cards-image-with-caption-link.css"
}