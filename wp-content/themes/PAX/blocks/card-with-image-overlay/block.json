{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 2,
    "name": "acf/card-with-image-overlay",
    "title": "Card with Image Overlay",
    "description": "A custom Card with Image Overlay block.",
    "category": "acf",
    "usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
    "icon": "index-card",
    "keywords": [ "cards", "with", "image", "overlay" ],
    "acf": {
        "mode": "preview",
        "renderTemplate": "card-with-image-overlay.php"
    },
    "attributes": {
        "align": {
            "type": "string",
            "default": "full"
        },
        "alignContent": {
            "type": "string",
            "default": "center left"
        }
    },
    "supports": {
        "color": {
            "background": false,
            "link": false,
            "text": true
        },
        "align": [
            "wide",
            "full"
        ],
        "anchor": true,
        "alignContent": "matrix",
        "alignText": true,
        "fullHeight": true,
        "html": true,
        "jsx": true,
        "multiple": true,
        "reusable": true
    },
    "style": "file:./card-with-image-overlay.css"
}