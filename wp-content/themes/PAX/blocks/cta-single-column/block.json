{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/cta-single-column",
	"title": "CTA - Single Column",
	"description": "A custom CTA - Single Column block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "block-default",
	"keywords": [ "cta", "single", "column"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "cta-single-column.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./cta-single-column.css"
}