{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/cards-with-image-pattern-repeater",
	"title": "Cards with Image + Pattern Repeater",
	"description": "A custom Cards with Image + Pattern Repeater block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "index-card",
	"keywords": [ "cards", "with", "image" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "cards-with-image-pattern-repeater.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./cards-with-image-pattern-repeater.css"
}