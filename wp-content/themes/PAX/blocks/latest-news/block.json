{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/latest-news",
	"title": "Latest News",
	"description": "A custom Latest News block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "admin-post",
	"keywords": [ "latest", "news"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "latest-news.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": "file:./latest-news.css"
}