{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/media-with-side-accordion",
	"title": "Media with Side Accordion",
	"description": "A custom Media with Side Accordion block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "image-flip-horizontal",
	"keywords": [ "media", "side", "accordion"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "media-with-side-accordion.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": ["file:./media-with-side-accordion.css"],
	"script": [ "media-with-side-accordion-script"]
}