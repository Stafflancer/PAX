{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/slider-cards-with-icons",
	"title": "Slider - Cards with Icons",
	"description": "A custom home page slider cards with icons block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "slides",
	"keywords": [ "slider", "cards", "icons" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "slider-cards-with-icons.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": [ "swiper-style", "file:./slider-cards-with-icons.css"],
	"script": [ "swiper-script", "slider-cards-with-icons-script"]
}