{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/slider-postcards",
	"title": "Slider - Postcards",
	"description": "A custom Slider - Postcards block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "slides",
	"keywords": [ "slider", "postcards"],
	"acf": {
		"mode": "preview",
		"renderTemplate": "slider-postcards.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": true,
		"reusable": true
	},
	"style": [ "swiper-style", "file:./slider-postcards.css"],
	"script":[ "swiper-script", "slider-postcards-script"]

}