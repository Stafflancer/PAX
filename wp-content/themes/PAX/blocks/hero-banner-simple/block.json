{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "acf/hero-banner-simple",
	"title": "Hero Banner - Simple",
	"description": "A custom Hero Banner - Simple block.",
	"category": "acf",
	"usesContext": [ "allowResize", "imageCrop", "fixedHeight" ],
	"icon": "admin-home",
	"keywords": [ "hero", "banner", "simple" ],
	"acf": {
		"mode": "preview",
		"renderTemplate": "hero-banner-simple.php"
	},
	"attributes": {
		"align": {
			"type": "string",
			"default": "full"
		},
		"alignContent": {
			"type": "string",
			"default": "center left"
		}
	},
	"supports": {
		"color": {
			"background": false,
			"link": false,
			"text": true
		},
		"align": [
			"wide",
			"full"
		],
		"anchor": true,
		"alignContent": "matrix",
		"alignText": true,
		"fullHeight": true,
		"html": true,
		"jsx": true,
		"multiple": false,
		"reusable": false
	},
	"style": "file:./hero-banner-simple.css"
}